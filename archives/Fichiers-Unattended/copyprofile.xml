            <RunSynchronous>
                <RunSynchronousCommand wcm:action="add">
                    <Description>Remove TPM check</Description>
                    <Path>reg add HKLM\System\Setup\LabConfig /v BypassTPMCheck /t reg_dword /d 0x00000001</Path>
                    <Order>1</Order>
                </RunSynchronousCommand>
                <RunSynchronousCommand wcm:action="add">
